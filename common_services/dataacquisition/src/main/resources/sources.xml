<?xml version="1.0" encoding="UTF-8"?>
<Import-Sources xmlns="http://escidoc.mpg.de/metadataprofile/schema/0.1/ImportSource" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:dc="http://purl.org/dc/elements/1.1/" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:eidt="http://escidoc.mpg.de/metadataprofile/schema/0.1/idtypes" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://escidoc.mpg.de/metadataprofile/schema/0.1/ImportSource explain-import.xsd">

	<Import-Source name="PubMedCentral">
		<dc:description>PubMed Central (PMC) is the U.S. National Institutes of Health (NIH) free digital archive of biomedical and life sciences journal literature.</dc:description>
		<dc:identifier xsi:type="dcterms:URI">http://www.pubmedcentral.nih.gov/</dc:identifier>
		<dc:format xsi:type="dcterms:IMT">text/xml</dc:format>
		<dc:format>UTF-8</dc:format>		
		<item-url>http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=GETID%26tool=pmcentrez%26rendertype=abstract</item-url>
		<source-identifier>pmcid</source-identifier>
		<source-identifier-example>pmcid:2581620</source-identifier-example>
		<source-oai_identifier>oai:pubmedcentral.nih.gov:</source-oai_identifier>
		<harvest-protocol>OAI-PMH</harvest-protocol>
		<retry-after>3</retry-after>
		<timeout>0</timeout>
		<number-of-tries>5</number-of-tries>
		<status>published</status>
			
		<!--Information about Metadata fetching -->
		<MD-fetch-settings>
			<MD-fetch-setting default="true">
				<dc:description>http://www.nlm.nih.gov/bsd/licensee/elements_descriptions.html</dc:description>
				<dc:identifier>http://www.pubmedcentral.nih.gov/oai/oai.cgi?verb=getRecord%26identifier=oai:pubmedcentral.nih.gov:GETID%26metadataPrefix=pmc_fm</dc:identifier>
				<dc:format>application/xml</dc:format>
				<label>PubMed Central</label>
				<name>pmc</name>
				<encoding>UTF-8</encoding>
				<fileType>.xml</fileType>
			</MD-fetch-setting>
			<MD-fetch-setting default="false">
				<dc:description>http://www.openarchives.org/OAI/2.0/oai_dc.xsd</dc:description>
				<dc:identifier>http://www.pubmedcentral.nih.gov/oai/oai.cgi?verb=getRecord%26identifier=oai:pubmedcentral.nih.gov:GETID%26metadataPrefix=oai_dc</dc:identifier>
				<dc:format>application/xml</dc:format>
				<label>oai dc</label>
				<name>oai_dc</name>
				<encoding>UTF-8</encoding>
				<fileType>.xml</fileType>
			</MD-fetch-setting>
		</MD-fetch-settings>

		<FT-fetch-settings>
			<FT-fetch-setting default="true">
				<dc:identifier>http://www.pubmedcentral.nih.gov/picrender.fcgi?artid=GETID%26blobtype=pdf</dc:identifier>
				<dc:format>application/pdf</dc:format>
				<label>PDF</label>
				<name>pdf</name>
				<encoding>*</encoding>
				<fileType>.pdf</fileType>
				<content-categorie>any-fulltext</content-categorie>
				<visibility>PUBLIC</visibility>
			</FT-fetch-setting>
		</FT-fetch-settings>
	</Import-Source>

	<Import-Source name="arXiv">
		<dc:description>ArXiv is an e-print service in the fields of physics, mathematics, non-linear science, computer science, quantitative biology and statistics.</dc:description>
		<dc:identifier xsi:type="dcterms:URI">http://arxiv.org</dc:identifier>
		<dc:format xsi:type="dcterms:IMT">text/xml</dc:format>
		<dc:format>UTF-8</dc:format>
		<item-url>http://arxiv.org/abs/GETID</item-url>
		<source-identifier>arxiv</source-identifier>
		<source-identifier-example>arXiv:0902.0003</source-identifier-example>
		<harvest-protocol>OAI-PMH</harvest-protocol>
		<retry-after>0</retry-after>
		<timeout>0</timeout>
		<number-of-tries>5</number-of-tries>
		<status>published</status>
		
		<!--Information about Metadata fetching -->
		<MD-fetch-settings>
			<MD-fetch-setting default="true">
				<dc:description>http://arxiv.org/help/prep</dc:description>
				<dc:identifier>http://export.arxiv.org/oai2/oai2?verb=GetRecord%26identifier=oai:arXiv.org:GETID%26metadataPrefix=arXiv</dc:identifier>
				<dc:format>application/xml</dc:format>
				<label>arXiv</label>
				<name>arXiv</name>
				<encoding>UTF-8</encoding>
				<fileType>.xml</fileType>
			</MD-fetch-setting>
			<MD-fetch-setting default="false">
				<dc:description>http://www.openarchives.org/OAI/2.0/oai_dc.xsd</dc:description>
				<dc:identifier>http://export.arxiv.org/oai2/oai2?verb=GetRecord%26identifier=oai:arXiv.org:GETID%26metadataPrefix=oai_dc</dc:identifier>
				<dc:format>application/xml</dc:format>
				<label>oai dc</label>
				<name>oai_dc</name>
				<encoding>UTF-8</encoding>
				<fileType>.xml</fileType>
			</MD-fetch-setting>
		</MD-fetch-settings>
		
		<!--Information about data fetching-->
		<FT-fetch-settings>
			<FT-fetch-setting default="true">
				<dc:identifier>http://export.arxiv.org/pdf/GETID</dc:identifier>
				<dc:format>application/pdf</dc:format>
				<label>PDF</label>
				<name>pdf</name>
				<encoding>*</encoding>
				<fileType>.pdf</fileType>
				<content-categorie>pre-print</content-categorie>
				<visibility>PUBLIC</visibility>
			</FT-fetch-setting>
			<FT-fetch-setting default="false">
				<dc:identifier>http://export.arxiv.org/ps/GETID</dc:identifier>
				<dc:format>application/gzip</dc:format>
				<label>Postscript</label>
				<name>ps</name>
				<encoding>*</encoding>
				<fileType>.ps.gz</fileType>
				<content-categorie>pre-print</content-categorie>
				<visibility>PUBLIC</visibility>
			</FT-fetch-setting>
		</FT-fetch-settings>
	</Import-Source>
	
	<Import-Source name="eSciDoc">
		<dc:description>eSciDoc is a eScience-aware knowledge and information management system.</dc:description>
		<dc:identifier xsi:type="dcterms:URI">http://www.escidoc-project.de</dc:identifier>
		<dc:format xsi:type="dcterms:IMT">text/xml</dc:format>
		<dc:format>UTF-8</dc:format>
		<item-url>http://localhost:8080/pubman/item/escidoc:GETID</item-url>
		<source-identifier>escidoc</source-identifier>
		<source-identifier-example>escidoc:176137</source-identifier-example>
		<harvest-protocol>ejb</harvest-protocol>
		<retry-after>0</retry-after>
		<timeout>0</timeout>
		<number-of-tries>5</number-of-tries>
		<status>published</status>
		
		<!--Information about Metadata fetching -->
		<MD-fetch-settings>
			<MD-fetch-setting default="true">
				<dc:description>http://colab.mpdl.mpg.de/mediawiki/ESciDoc_Metadata</dc:description>
				<dc:identifier>http://www.ThisIsInternallyHandled.info</dc:identifier>
				<dc:format>application/xml</dc:format>
				<label>ESciDoc</label>
				<name>escidoc</name>
				<encoding>UTF-8</encoding>
				<fileType>.xml</fileType>
			</MD-fetch-setting>
		</MD-fetch-settings>
		
		<!--Information about data fetching-->
		<FT-fetch-settings>
		</FT-fetch-settings>
	</Import-Source>
	
	<Import-Source name="eSciDoc_QA">
		<dc:description>eSciDoc items on the QA server</dc:description>
		<dc:identifier xsi:type="dcterms:URI">http://qa-pubman.mpdl.mpg.de:8080/pubman</dc:identifier>
		<dc:format xsi:type="dcterms:IMT">text/xml</dc:format>
		<dc:format>UTF-8</dc:format>
		<item-url>http://qa-pubman.mpdl.mpg.de:8080/pubman/item/escidoc:GETID</item-url>
		<source-identifier>escidocQA</source-identifier>
		<source-identifier-example>escidocQA:176137</source-identifier-example>
		<harvest-protocol>http</harvest-protocol>
		<retry-after>0</retry-after>
		<timeout>0</timeout>
		<number-of-tries>5</number-of-tries>
		<status>published</status>
		
		<!--Information about Metadata fetching -->
		<MD-fetch-settings>
			<MD-fetch-setting default="true">
				<dc:identifier>http://qa-pubman.mpdl.mpg.de:8080/search/SearchAndExport?cqlQuery=escidoc.any-identifier=%22escidoc:GETID%22%26exportFormat=BIBTEX</dc:identifier>
				<dc:format>text/plain</dc:format>
				<label>BibTex</label>
				<name>bibtex</name>
				<encoding>UTF-8</encoding>
				<fileType>.bib</fileType>
			</MD-fetch-setting>
			<MD-fetch-setting default="false">
				<dc:identifier>http://qa-pubman.mpdl.mpg.de:8080/search/SearchAndExport?cqlQuery=escidoc.any-identifier=%22escidoc:GETID%26%22exportFormat=ENDNOTE</dc:identifier>
				<dc:format>text/plain</dc:format>
				<label>EndNote</label>
				<name>endnote</name>
				<encoding>UTF-8</encoding>
				<fileType>.txt</fileType>
			</MD-fetch-setting>
		</MD-fetch-settings>
		
		<!--Information about data fetching-->
		<FT-fetch-settings>
		</FT-fetch-settings>
	</Import-Source>
	
</Import-Sources>
