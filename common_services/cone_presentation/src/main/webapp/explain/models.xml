<models>
	<model>
		<name>journals</name>
		<aliases>
			<alias>jnar</alias>
			<alias>journals</alias>
		</aliases>
		<description>Journal Names</description>
		<primary-identifier control="true" generate-cone-id="false" subject-prefix="urn:cone:sfx:" identifier-prefix="">http://purl.org/dc/terms/identifier</primary-identifier>
		<predicates>
			<predicate name="Journal Name" value="http://purl.org/dc/elements/1.1/title" multiple="false" mandatory="true" localized="false" searchable="true"/>
			<predicate name="Alternative Journal Name" value="http://purl.org/dc/terms/alternative" multiple="true" mandatory="false" localized="false" searchable="true"/>
			<predicate name="SFX-ID" value="http://purl.org/dc/terms/identifier" multiple="false" mandatory="true" localized="false" searchable="true"/>
			<predicate name="Identifier" value="http://purl.org/dc/elements/1.1/identifier" multiple="true" mandatory="false" localized="false" generateObject="true" searchable="true">
				<predicate name="Value" value="http://www.w3.org/1999/02/22-rdf-syntax-ns#/value" multiple="false" mandatory="true" localized="false" searchable="true"/>
				<predicate name="ID Type" value="http://escidoc.mpg.de/idtype" multiple="false" mandatory="true" localized="false" searchable="false"/>
			</predicate>
			<predicate name="Publisher" value="http://purl.org/dc/elements/1.1/publisher" multiple="false" mandatory="false" localized="false" searchable="true"/>
			<predicate name="Publisher Place" value="http://purl.org/dc/terms/publisher" multiple="false" mandatory="false" localized="true" searchable="true"/>
		</predicates>
		<result>
			<pattern>
&lt;http://purl.org/dc/elements/1.1/title&gt;
OR{&lt;http://purl.org/dc/elements/1.1/publisher&gt;,&lt;http://purl.org/dc/terms/publisher&gt;}:; 
&lt;http://purl.org/dc/elements/1.1/publisher&gt;
AND{&lt;http://purl.org/dc/elements/1.1/publisher&gt;,&lt;http://purl.org/dc/terms/publisher&gt;}:, 
&lt;http://purl.org/dc/terms/publisher&gt;
			</pattern>
		</result>
		<samples>
			<sample>
				<description>Find journal names matching 'bio', e.g. 'Microbiology', 'Biometrics'.</description>
				<method>
					<name>query</name>
					<parameters>
						<parameter name="q">bio</parameter>
					</parameters>
				</method>
			</sample>
			<sample>
				<description>Retrieve details about the "Journal of Endocrinological Investigation".</description>
				<method>
					<name>details</name>
					<parameters>
						<parameter name="id">urn:sfx:954925529888</parameter>
					</parameters>
				</method>
			</sample>
		</samples>
		<example_page>../jquery_journals.jsp</example_page>
	</model>
	<model>
		<name>languages</name>
		<aliases>
			<alias>lang</alias>
			<alias>languages</alias>
		</aliases>
		<primary-identifier control="true" generate-cone-id="false" subject-prefix="urn:cone:iso639:" identifier-prefix="">http://purl.org/dc/elements/1.1/identifier</primary-identifier>
		<predicates>
			<predicate name="Name" value="http://purl.org/dc/elements/1.1/title" multiple="true" mandatory="true" localized="true" searchable="true"/>
			<predicate name="ISO639 Code" value="http://purl.org/dc/elements/1.1/identifier" multiple="false" mandatory="true" localized="false" searchable="false"/>
		</predicates>
		<result>
			<pattern>
				&lt;http://purl.org/dc/elements/1.1/title&gt;
			</pattern>
		</result>
		<description>Languages</description>
		<samples>
			<sample>
				<description>Find languages matching 'mal', given in french.</description>
				<method>
					<name>query</name>
					<parameters>
						<parameter name="q">mal</parameter>
						<parameter name="lang">fr</parameter>
					</parameters>
				</method>
			</sample>
		</samples>
		<example_page>../jquery_languages.jsp</example_page>
	</model>
	<model>
		<name>persons</name>
		<aliases>
			<alias>authors</alias>
			<alias>persons</alias>
		</aliases>
		<primary-identifier control="false" generate-cone-id="true" subject-prefix="urn:cone:"/>
		<predicates>
			<predicate name="Complete Name" value="http://purl.org/dc/elements/1.1/title" multiple="false" mandatory="true" localized="true" searchable="true"/>
			<!-- <predicate name="Cone-ID" value="http://purl.org/dc/elements/1.1/identifier" multiple="false" mandatory="true" localized="false"/> -->
			<predicate name="Alternative Name" value="http://purl.org/dc/terms/alternative" multiple="true" mandatory="false" localized="true" searchable="true"/>
			<predicate name="Family Name" value="http://xmlns.com/foaf/0.1/familyname" multiple="false" mandatory="true" localized="true" searchable="true"/>
			<predicate name="Given Name" value="http://xmlns.com/foaf/0.1/givenname" multiple="false" mandatory="true" localized="true" searchable="true"/>
			<predicate name="Photo-URL" value="http://xmlns.com/foaf/0.1/depiction" multiple="false" mandatory="false" localized="false" searchable="false"/>
			<predicate name="Identifier" value="http://purl.org/dc/elements/1.1/identifier" multiple="true" mandatory="false" localized="false" generateObject="true" searchable="true">
				<predicate name="Value" value="http://www.w3.org/1999/02/22-rdf-syntax-ns#/value" multiple="false" mandatory="true" localized="false" searchable="true"/>
				<predicate name="ID Type" value="http://escidoc.mpg.de/idtype" multiple="false" mandatory="true" localized="false" searchable="false"/>
			</predicate>
			<predicate name="Researcher URL" value="http://xmlns.com/foaf/0.1/homepage" multiple="true" mandatory="false" localized="true" generateObject="true" searchable="false">
				<predicate name="Value" value="http://www.w3.org/1999/02/22-rdf-syntax-ns#/value" multiple="false" mandatory="true" localized="false" searchable="false"/>
				<predicate name="Description" value="http://purl.org/dc/elements/1.1/description" multiple="false" mandatory="true" localized="true" searchable="false"/>
			</predicate>
			<predicate name="Keywords of research fields and interests" value="http://purl.org/dc/terms/subject" multiple="false" mandatory="false" localized="true" searchable="false"/>
			<predicate name="Subject (DDC)" value="http://purl.org/dc/elements/1.1/subject" multiple="true" mandatory="false" localized="true" resourceModel="ddc" searchable="false"/>
			<predicate name="Degree" value="http://escidoc.mpg.de/degree" multiple="false" mandatory="false" localized="true" searchable="false"/>
			<predicate name="Awards" value="http://escidoc.mpg.de/award" multiple="false" mandatory="false" localized="true" searchable="false"/>
			<predicate name="Position" value="http://escidoc.mpg.de/position" multiple="true" mandatory="false" localized="true" generateObject="true" searchable="true">
				<predicate name="Organization" value="http://escidoc.mpg.de/organization" multiple="false" mandatory="false" localized="true" searchable="true"/>
				<predicate name="eSciDoc-ID" value="http://purl.org/dc/elements/1.1/identifier" multiple="false" mandatory="false" localized="false" searchable="false"/>
				<predicate name="Position Appellation" value="http://escidoc.mpg.de/position-name" multiple="false" mandatory="false" localized="true" searchable="false"/>
				<predicate name="From" value="http://purl.org/dc/elements/1.1/start-date" multiple="false" mandatory="false" localized="false" searchable="false"/>
				<predicate name="Until" value="http://purl.org/dc/elements/1.1/end-date" multiple="false" mandatory="false" localized="false" searchable="false"/>
				<predicate name="E-mail Address" value="http://xmlns.com/foaf/0.1/email" multiple="false" mandatory="false" localized="false" searchable="false"/>
			</predicate>
			<!-- <predicate name="Other affiliations" value="urn:cone:other-ou" multiple="false" mandatory="false" localized="true"/> -->
			<predicate name="E-mail Address" value="http://xmlns.com/foaf/0.1/email" multiple="false" mandatory="false" localized="false" searchable="false"/>
			<predicate name="Telephone" value="http://xmlns.com/foaf/0.1/tel" multiple="false" mandatory="false" localized="false" searchable="false"/>
			<predicate name="Actual Status" value="http://escidoc.mpg.de/person-activity-status" multiple="false" mandatory="false" localized="false" searchable="false"/>
			<predicate name="Last Modification Date" value="http://purl.org/dc/terms/modified" multiple="false" mandatory="true" localized="false" searchable="false" default="de.mpg.escidoc.services.cone.util.EditHelper.getCurrentDate" overwrite="true" event="onsave" modify="false"/>
			<predicate name="Creation Date" value="http://purl.org/dc/terms/created" multiple="false" mandatory="true" localized="false" searchable="false" default="de.mpg.escidoc.services.cone.util.EditHelper.getCurrentDate" overwrite="false" event="onsave" modify="false"/>
			<predicate name="Modified By" value="http://escidoc.mpg.de/modified-by" multiple="false" mandatory="true" localized="false" searchable="false" default="de.mpg.escidoc.services.cone.util.EditHelper.getCurrentUser" overwrite="true" event="onsave" modify="false"/>
			<predicate name="Created By" value="http://escidoc.mpg.de/created-by" multiple="false" mandatory="true" localized="false" searchable="false" default="de.mpg.escidoc.services.cone.util.EditHelper.getCurrentUser" overwrite="false" event="onsave" modify="false"/>
		</predicates>
		<result>
			<pattern>
&lt;http://purl.org/dc/elements/1.1/title&gt;
AND{&lt;http://purl.org/dc/elements/1.1/title&gt;,&lt;http://escidoc.mpg.de/position|http://escidoc.mpg.de/organization&gt;}: (
OR{&lt;http://purl.org/dc/elements/1.1/title&gt;}:&lt;http://escidoc.mpg.de/position|http://escidoc.mpg.de/organization&gt;
AND{&lt;http://purl.org/dc/elements/1.1/title&gt;,&lt;http://escidoc.mpg.de/position|http://escidoc.mpg.de/organization&gt;}:)
			</pattern>
			<pattern>
&lt;http://purl.org/dc/terms/alternative&gt;
AND{&lt;http://purl.org/dc/terms/alternative&gt;,&lt;http://escidoc.mpg.de/position|http://escidoc.mpg.de/organization&gt;}: (
OR{&lt;http://purl.org/dc/terms/alternative&gt;}:&lt;http://escidoc.mpg.de/position|http://escidoc.mpg.de/organization&gt;
AND{&lt;http://purl.org/dc/terms/alternative&gt;,&lt;http://escidoc.mpg.de/position|http://escidoc.mpg.de/organization&gt;}:)
			</pattern>
		</result>
		<description>Persons</description>
		<samples>
			<sample>
				<description>Find persons matching 'Berkum'.</description>
				<method>
					<name>query</name>
					<parameters>
						<parameter name="q">Berkum</parameter>
					</parameters>
				</method>
			</sample>
		</samples>
	</model>
	<model>
		<name>ddc</name>
		<aliases>
			<alias>ddc</alias>
		</aliases>
		<primary-identifier control="true" generate-cone-id="false" subject-prefix="urn:cone:ddc:" identifier-prefix="">http://purl.org/dc/elements/1.1/identifier</primary-identifier>
		<predicates>
			<predicate name="Name" value="http://purl.org/dc/elements/1.1/title" multiple="true" mandatory="true" localized="true" searchable="true"/>
			<predicate name="DDC Code" value="http://purl.org/dc/elements/1.1/identifier" multiple="false" mandatory="true" localized="false" searchable="true"/>
		</predicates>
		<result>
			<pattern>
&lt;http://purl.org/dc/elements/1.1/identifier&gt;
 - 
&lt;http://purl.org/dc/elements/1.1/title&gt;
			</pattern>
		</result>
		<description>DDC Subjects</description>
		<samples>
			<sample>
				<description>Find subjects matching 'Computer', given in english.</description>
				<method>
					<name>query</name>
					<parameters>
						<parameter name="q">Computer</parameter>
						<parameter name="lang">en</parameter>
					</parameters>
				</method>
			</sample>
		</samples>
		<!-- <example_page>../jquery_languages.jsp</example_page> -->
	</model>
	<model>
		<name>mimetypes</name>
		<aliases>
			<alias>mimetypes</alias>
		</aliases>
		<primary-identifier control="true" generate-cone-id="false" subject-prefix="urn:cone:mimetype:" identifier-prefix="">http://purl.org/dc/elements/1.1/title</primary-identifier>
		<predicates>
			<predicate name="Type" value="http://purl.org/dc/elements/1.1/title" multiple="false" mandatory="true" localized="false" searchable="true"/>
		</predicates>
		<result>
			<pattern>
&lt;http://purl.org/dc/elements/1.1/title&gt;
			</pattern>
		</result>
		<description>IANA Mimetypes</description>
		<samples>
			<sample>
				<description>Find subjects matching 'Computer', given in english.</description>
				<method>
					<name>query</name>
					<parameters>
						<parameter name="q">xml</parameter>
					</parameters>
				</method>
			</sample>
		</samples>
		<!-- <example_page>../jquery_languages.jsp</example_page> -->
	</model>
	<model>
		<name>escidocmimetypes</name>
		<aliases>
			<alias>escidocmimetypes</alias>
		</aliases>
		<primary-identifier control="true" generate-cone-id="false" subject-prefix="urn:cone:escidocmimetype:" identifier-prefix="">http://purl.org/dc/elements/1.1/relation</primary-identifier>
		<predicates>
			<predicate name="Type" value="http://purl.org/dc/elements/1.1/relation" multiple="false" mandatory="true" localized="false" resourceModel="mimetypes" searchable="true"/>
			<predicate name="File Ending" value="urn:cone:suffix" multiple="false" mandatory="false" localized="false" searchable="true" default=".abc" overwrite="false"/>
		</predicates>
		<result>
			<pattern>
&lt;http://purl.org/dc/elements/1.1/relation|http://purl.org/dc/elements/1.1/title&gt;
			</pattern>
		</result>
		<description>Allowed eSciDoc Mimetypes</description>
		<samples>
			<sample>
				<description>Return the whole list of allowed mimetypes.</description>
				<method>
					<name>all</name>
				</method>
			</sample>
		</samples>
		<!-- <example_page>../jquery_languages.jsp</example_page> -->
	</model>
	<model>
		<name>cclicences</name>
		<aliases>
			<alias>cclicences</alias>
		</aliases>
		<primary-identifier control="true" generate-cone-id="false" subject-prefix="urn:cone:cclicence:" identifier-prefix="http://creativecommons.org/licenses/">http://purl.org/dc/elements/1.1/identifier</primary-identifier>
		<predicates>
			<predicate name="Version" value="urn:cone:version" multiple="false" mandatory="true" localized="false" searchable="true"/>
			<predicate name="Name" value="http://purl.org/dc/elements/1.1/title" multiple="false" mandatory="true" localized="false" searchable="true"/>
			<predicate name="URL" value="http://purl.org/dc/elements/1.1/identifier" multiple="false" mandatory="true" localized="false" searchable="true"/>
			<predicate name="Image-URL" value="http://xmlns.com/foaf/0.1/depiction" multiple="false" mandatory="false" localized="false" searchable="true"/>
			<predicate name="Commercial" value="urn:cone:commercial" multiple="false" mandatory="true" localized="false" searchable="true"/>
			<predicate name="Derivatives" value="urn:cone:derivatives" multiple="false" mandatory="true" localized="false" searchable="true"/>
			<predicate name="Jurisdiction" value="urn:cone:jurisdiction" multiple="false" mandatory="true" localized="false" searchable="true"/>
			<predicate name="Translation" value="urn:cone:translation" multiple="true" mandatory="false" localized="true" searchable="false" generateObject="true">
				<predicate name="Name" value="http://purl.org/dc/elements/1.1/title" multiple="false" mandatory="true" localized="false" searchable="true"/>
				<predicate name="URL" value="http://purl.org/dc/elements/1.1/identifier" multiple="false" mandatory="true" localized="false" searchable="false"/>
				<!-- <predicate name="Language" value="http://purl.org/dc/elements/1.1/title" multiple="false" mandatory="true" localized="false" searchable="false"/> -->
				<predicate name="Locale" value="urn:cone:locale" multiple="false" mandatory="true" localized="false" searchable="false"/>
			</predicate>
		</predicates>
		<result>
			<pattern>
&lt;http://purl.org/dc/elements/1.1/title&gt;
			</pattern>
		</result>
		<description>CreativeCommons licences</description>
		<samples>
			<sample>
				<description>Return the whole list of CreativeCommons licences.</description>
				<method>
					<name>all</name>
				</method>
			</sample>
		</samples>
	</model>
	<config>
		<default-namespace uri="http://purl.org/dc/elements/1.1/" prefix="dc"/>
		<default-namespace uri="http://purl.org/dc/terms/" prefix="dcterms"/>
		<default-namespace uri="http://xmlns.com/foaf/0.1/" prefix="foaf"/>
		<default-namespace uri="http://escidoc.mpg.de/" prefix="escidoc"/>
		<default-namespace uri="http://www.w3.org/1999/02/22-rdf-syntax-ns#" prefix="rdf"/>
	</config>
</models>