<models>
	<model>
		<name>journals</name>
		<aliases>
			<alias>jnar</alias>
			<alias>journals</alias>
		</aliases>
		<description>Journal Names</description>
		<primary-identifier generate="false" prefix="urn:sfx:">http://purl.org/dc/elements/1.1/identifier</primary-identifier>
		<predicates>
			<predicate name="Journal Name" value="http://purl.org/dc/elements/1.1/title" multiple="false" mandatory="true" localized="false"/>
			<predicate name="Alternative Journal Name" value="http://purl.org/dc/terms/alternative" multiple="true" mandatory="false" localized="false"/>
			<predicate name="SFX-ID" value="http://purl.org/dc/elements/1.1/identifier" multiple="false" mandatory="true" localized="false"/>
			<predicate name="Identifier" value="http://purl.org/dc/terms/identifier" multiple="true" mandatory="false" localized="false"/>
			<predicate name="Publisher" value="http://purl.org/dc/elements/1.1/publisher" multiple="false" mandatory="false" localized="false"/>
			<predicate name="Publisher Place" value="http://purl.org/dc/terms/publisher" multiple="false" mandatory="false" localized="true"/>
		</predicates>
		<result>
			<pattern>
				&lt;http://purl.org/dc/elements/1.1/title&gt;
				OR{&lt;http://purl.org/dc/elements/1.1/publisher&gt;,&lt;http://purl.org/dc/terms/publisher&gt;}:; 
				&lt;http://purl.org/dc/elements/1.1/publisher&gt;
				AND{&lt;http://purl.org/dc/elements/1.1/publisher&gt;,&lt;http://purl.org/dc/terms/publisher&gt;}:, 
				&lt;http://purl.org/dc/terms/publisher&gt;
			</pattern>
		</result>
		<samples>
			<sample>
				<description>Find journal names matching 'bio', e.g. 'Microbiology', 'Biometrics'.</description>
				<method>
					<name>query</name>
					<parameters>
						<parameter name="q">bio</parameter>
					</parameters>
				</method>
			</sample>
			<sample>
				<description>Retrieve details about the "Journal of Endocrinological Investigation".</description>
				<method>
					<name>details</name>
					<parameters>
						<parameter name="id">urn:sfx:954925529888</parameter>
					</parameters>
				</method>
			</sample>
		</samples>
		<example_page>../jquery_journals.jsp</example_page>
	</model>
	<model>
		<name>languages</name>
		<aliases>
			<alias>lang</alias>
			<alias>languages</alias>
		</aliases>
		<primary-identifier generate="false" prefix="urn:iso639:">http://purl.org/dc/elements/1.1/identifier</primary-identifier>
		<predicates>
			<predicate name="Name" value="http://purl.org/dc/elements/1.1/title" multiple="true" mandatory="true" localized="true"/>
			<predicate name="ISO639 Code" value="http://purl.org/dc/elements/1.1/identifier" multiple="false" mandatory="true" localized="false"/>
		</predicates>
		<result>
			<pattern>
				&lt;http://purl.org/dc/elements/1.1/title&gt;
			</pattern>
		</result>
		<description>Languages</description>
		<samples>
			<sample>
				<description>Find languages matching 'mal', given in french.</description>
				<method>
					<name>query</name>
					<parameters>
						<parameter name="q">mal</parameter>
						<parameter name="lang">fr</parameter>
					</parameters>
				</method>
			</sample>
		</samples>
		<example_page>../jquery_languages.jsp</example_page>
	</model>
	<model>
		<name>persons</name>
		<aliases>
			<alias>authors</alias>
			<alias>persons</alias>
		</aliases>
		<primary-identifier generate="true" prefix="urn:cone:">http://purl.org/dc/elements/1.1/identifier</primary-identifier>
		<predicates>
			<predicate name="Complete Name" value="http://purl.org/dc/elements/1.1/title" multiple="false" mandatory="true" localized="false"/>
			<predicate name="Cone-ID" value="http://purl.org/dc/elements/1.1/identifier" multiple="false" mandatory="true" localized="false"/>
			<predicate name="Alternative Name" value="http://purl.org/dc/terms/alternative" multiple="true" mandatory="false" localized="false"/>
			<predicate name="Surname" value="urn:cone:surname" multiple="false" mandatory="false" localized="false"/>
			<predicate name="Given Name" value="urn:cone:givenname" multiple="false" mandatory="false" localized="false"/>
			<predicate name="Photo-URL" value="urn:cone:photo" multiple="false" mandatory="false" localized="false"/>
			<predicate name="Identifier" value="http://purl.org/dc/terms/identifier" multiple="true" mandatory="false" localized="false"/>
			<predicate name="Researcher URL" value="urn:cone:url" multiple="true" mandatory="false" localized="true"/>
			<predicate name="Keywords of research fields and interests" value="urn:cone:keyword" multiple="true" mandatory="false" localized="true"/>
			<predicate name="Subject (DDC)" value="urn:cone:ddc" multiple="false" mandatory="false" localized="true"/>
			<predicate name="Degree" value="urn:cone:degree" multiple="false" mandatory="false" localized="true"/>
			<predicate name="Current position" value="urn:cone:position" multiple="false" mandatory="false" localized="true"/>
			<predicate name="Awards" value="urn:cone:awards" multiple="false" mandatory="false" localized="true"/>
			<predicate name="Organizational Unit" value="urn:cone:ou" multiple="false" mandatory="false" localized="true"/>
			<predicate name="Other affiliations" value="urn:cone:other-ou" multiple="false" mandatory="false" localized="true"/>
			<predicate name="E-mail Address" value="urn:cone:email" multiple="true" mandatory="false" localized="false"/>
			<predicate name="Telephone" value="urn:cone:phone" multiple="false" mandatory="false" localized="false"/>
		</predicates>
		<result>
			<pattern>
				&lt;http://purl.org/dc/elements/1.1/title&gt;
			</pattern>
			<pattern>
				&lt;http://purl.org/dc/terms/alternative&gt;
			</pattern>
		</result>
		<description>Persons</description>
		<samples>
			<sample>
				<description>Find persons matching 'Berkum'.</description>
				<method>
					<name>query</name>
					<parameters>
						<parameter name="q">Berkum</parameter>
					</parameters>
				</method>
			</sample>
		</samples>
	</model>
	<model>
		<name>ddc</name>
		<aliases>
			<alias>ddc</alias>
		</aliases>
		<primary-identifier generate="false" prefix="http://dewey.info/class/">http://purl.org/dc/elements/1.1/identifier</primary-identifier>
		<predicates>
			<predicate name="Name" value="http://purl.org/dc/elements/1.1/title" multiple="true" mandatory="true" localized="true"/>
			<predicate name="DDC Code" value="http://purl.org/dc/elements/1.1/identifier" multiple="false" mandatory="true" localized="false"/>
		</predicates>
		<result>
			<pattern>
				&lt;http://purl.org/dc/elements/1.1/title&gt;
			</pattern>
		</result>
		<description>DDC Subjects</description>
		<samples>
			<sample>
				<description>Find subjects matching 'Computer', given in english.</description>
				<method>
					<name>query</name>
					<parameters>
						<parameter name="q">Computer</parameter>
						<parameter name="lang">en</parameter>
					</parameters>
				</method>
			</sample>
		</samples>
		<!-- <example_page>../jquery_languages.jsp</example_page> -->
	</model>
</models>