<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>COntrolled Named Entities</display-name>
  
  <servlet>
  	<servlet-name>JQueryConeServlet</servlet-name>
  	<servlet-class>de.mpg.escidoc.services.cone.web.JQueryConeServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>JQueryConeServlet</servlet-name>
  	<url-pattern>/jquery/*</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
  	<welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
</web-app>
