<foxml:datastreamVersion ID="RELS-EXT.5" LABEL="RELS_EXT DATASTREAM" CREATED="2013-12-05T12:47:55.566Z" MIMETYPE="text/xml" SIZE="2336">
<foxml:xmlContent>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <rdf:Description rdf:about="info:fedora/escidoc:1859310">
    <srel:component xmlns:srel="http://escidoc.de/core/01/structural-relations/" rdf:resource="info:fedora/escidoc:1859315"></srel:component>
    <system:build xmlns:system="http://escidoc.de/core/01/system/">1.3.9</system:build>
