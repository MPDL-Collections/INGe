				<pub:publication type="http://purl.org/eprint/type/Thesis">
					<eterms:creator role="http://www.loc.gov/loc.terms/relators/AUT">
						<person:person>
							<eterms:family-name>Wenz</eterms:family-name>
							<eterms:given-name>Andre Niklas</eterms:given-name>
                     <!-- Case 3 -->
							<organization:organization>
