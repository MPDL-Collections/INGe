						</url>
						<url>
							${escidoc.framework_access.framework.url}/axis/services/ReportDefinitionHandlerService?wsdl
						</url>
						<url>
							${escidoc.framework_access.framework.url}/axis/services/ReportHandlerService?wsdl
						</url>
						<url>
							${escidoc.framework_access.framework.url}/axis/services/ContainerHandlerService?wsdl
						</url>
						<url>
							${escidoc.framework_access.framework.url}/srw/search?wsdl
						</url>
					</urls>
					<mappings>
						<mapping>
							<namespace>
								http://www.escidoc.de/services/OrganizationalUnitHandlerService/0.1
							</namespace>
							<targetPackage>
								de.fiz.escidoc.oum
							</targetPackage>
						</mapping>
						<mapping>
							<namespace>
								http://www.escidoc.de/services/UserManagementWrapperService/0.1
							</namespace>
							<targetPackage>
								de.fiz.escidoc.um
							</targetPackage>
						</mapping>
						<mapping>
							<namespace>
								http://www.escidoc.de/services/UserAccountHandlerService/0.1
							</namespace>
							<targetPackage>
								de.fiz.escidoc.um
							</targetPackage>
						</mapping>
						<mapping>
