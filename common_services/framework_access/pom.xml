								http://www.escidoc.de/services/ReportDefinitionHandlerService/0.1
							</namespace>
							<targetPackage>
								de.fiz.escidoc.sm
							</targetPackage>
						</mapping>
						<mapping>
							<namespace>
								http://www.escidoc.de/services/ReportHandlerService/0.1
							</namespace>
							<targetPackage>
								de.fiz.escidoc.sm
							</targetPackage>
						</mapping>
						<mapping>
							<namespace>
								http://www.escidoc.de/services/ContainerHandlerService/0.1
							</namespace>
							<targetPackage>
								de.fiz.escidoc.om
							</targetPackage>
						</mapping>
						<mapping>
							<namespace>
								http://www.escidoc.de/srw/search/0.1
							</namespace>
							<targetPackage>
								de.fiz.escidoc.search
							</targetPackage>
						</mapping>
					</mappings>
					<testCases>false</testCases>
					<serverSide>false</serverSide>
					<subPackageByFileName>true</subPackageByFileName>
					<useEmitter>false</useEmitter>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-surefire-plugin</artifactId>
				<configuration>
					<test>TestFrameworkAccess</test>
				</configuration>
			</plugin>
		</plugins>
	</build>
