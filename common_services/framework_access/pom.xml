	<modelVersion>4.0.0</modelVersion>
	<groupId>de.mpg.escidoc.services</groupId>
	<artifactId>framework_access</artifactId>
	<name>framework_access</name>
	<version>1.3-SNAPSHOT</version>
	<scm>
		<connection>scm:svn:https://zim02.gwdg.de/repos/common/trunk/common_services/framework_access</connection>
		<developerConnection>scm:svn:https://zim02.gwdg.de/repos/common/trunk/common_services/framework_access</developerConnection>
