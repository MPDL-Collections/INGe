
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<parent>
		<artifactId>common_services</artifactId>
		<groupId>de.mpg.escidoc.services</groupId>
		<version>1.0.44-SNAPSHOT</version>
	</parent>
	<modelVersion>4.0.0</modelVersion>
	<groupId>de.mpg.escidoc.services</groupId>
	<artifactId>pid_cache_presentation</artifactId>
	<name>pid_cache_presentation</name>
	<version>0.1.7-SNAPSHOT</version>
	<packaging>war</packaging>
	
	<scm>
		<connection>scm:svn:https://zim02.gwdg.de/repos/common/trunk/common_services/pid_cache_presentation</connection>
		<developerConnection>scm:svn:https://zim02.gwdg.de/repos/common/trunk/common_services/pid_cache_presentation</developerConnection>
		<url>https://zim02.gwdg.de/repos/common/</url>
	</scm>
	
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-eclipse-plugin</artifactId>
				<configuration>
					<wtpversion>1.5</wtpversion>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
				</configuration>
			</plugin>
			<plugin>
				<artifactId>maven-surefire-plugin</artifactId>
				<configuration>
					<skip>true</skip>
				</configuration>
				<executions>
					<execution>
						<phase>integration-test</phase>
						<goals>
							<goal>test</goal>
						</goals>
						<configuration>
							<skip>true</skip>
						</configuration>
					</execution>
				</executions>
			</plugin>
		</plugins>
	</build>
	
	<dependencies>
		<dependency>
            <groupId>de.mpg.escidoc.services</groupId>
            <artifactId>framework_access</artifactId>
            <version>1.0.47-SNAPSHOT</version>
            <scope>provided</scope>
        </dependency>
		<dependency>
            <groupId>de.mpg.escidoc.services</groupId>
            <artifactId>common_logic</artifactId>
            <version>1.1.44-SNAPSHOT</version>
            <scope>provided</scope>
        </dependency>
		<dependency>
			<groupId>commons-httpclient</groupId>
			<artifactId>commons-httpclient</artifactId>
			<version>3.0.1</version>
			<scope>provided</scope>
		</dependency>
		<dependency>
			<groupId>javaee</groupId>
			<artifactId>javaee</artifactId>
			<version>1.5</version>
			<scope>provided</scope>
		</dependency>
	</dependencies>
</project>