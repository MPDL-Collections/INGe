<export-formats
	xmlns="http://escidoc.mpg.de/export/schema/0.1/explain-formats"
	xmlns:dc="http://purl.org/dc/elements/1.1/" 
	xmlns:dcterms="http://purl.org/dc/terms/"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://escidoc.mpg.de/export/schema/0.1/explain-formats explain-export-formats.xsd">
	<export-format type="layout" context="PubMan">
		<dc:title>APA Citation Style</dc:title>
		<dc:identifier>APA</dc:identifier>
		<output-format>
			<dc:title>pdf</dc:title>
			<dc:format xsi:type="dcterms:IMT">application/pdf</dc:format>
		</output-format>
		<output-format>
			<dc:title>rtf</dc:title>
			<dc:format xsi:type="dcterms:IMT">application/rtf</dc:format>
		</output-format>
		<output-format>
			<dc:title>html</dc:title>
			<dc:format xsi:type="dcterms:IMT">text/html</dc:format>
		</output-format>
		<output-format>
			<dc:title>odt</dc:title>
			<dc:format xsi:type="dcterms:IMT">application/vnd.oasis.opendocument.text</dc:format>
		</output-format>
		<output-format>
			<dc:title>snippet</dc:title>
			<dc:format xsi:type="dcterms:IMT">application/xml</dc:format>
		</output-format>
		<output-format>
			<dc:title>escidoc_snippet</dc:title>
			<dc:format xsi:type="dcterms:IMT">application/xml</dc:format>
		</output-format>
		<output-format>
			<dc:title>txt</dc:title>
			<dc:format xsi:type="dcterms:IMT">text/plain</dc:format>
		</output-format>
		<dc:creator>vmakarenko</dc:creator>
	</export-format>
