<?xml version="1.0" encoding="UTF-8"?>
<font-styles-collection name="Default">

  <font-style name="NORMAL"
    def="true"
    font-name="Arial"
    font-size="12"
    fore-color="black"
    back-color="white"
    is-bold="false"
    is-italic="false"
    pdf-font-name="ARIAL.TTF"
    is-underline="false"
    is-strike-through="false"/>

  <font-style name="ARIAL14"
    font-name="Arial"
    font-size="14"
    fore-color="black"
    back-color="white"
    is-bold="false"
    is-italic="false"
    pdf-font-name="ARIAL.TTF"
    is-underline="false"
    is-strike-through="false"/>

  <font-style name="BOLD"
    fore-color="black"
    back-color="white"
    is-bold="true"
    is-italic="false"
    pdf-font-name="ARIALBD.TTF"
