    is-strike-through="false"/>

  <font-style name="ITALIC"
    fore-color="black"
    back-color="white"
    is-bold="false"
    is-italic="true"
    pdf-font-name="ARIALBI.TTF"
    is-underline="false"
    is-strike-through="false"/>

  <font-style name="UNDERLINE_ITALIC"
    fore-color="black"
    back-color="white"
    is-bold="false"
    is-italic="true"
    pdf-font-name="ARIAL.TTF"
    is-underline="true"
    is-strike-through="false"/>

  <font-style name="BOLD_ITALIC"
    fore-color="black"
    back-color="white"
    is-bold="true"
    is-italic="true"
    pdf-font-name="ARIALBI.TTF"
    is-underline="true"
    is-strike-through="false"/>

