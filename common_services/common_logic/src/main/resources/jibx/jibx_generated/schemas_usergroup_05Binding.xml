<binding xmlns:tns="http://www.escidoc.de/schemas/usergroup/0.5" xmlns:ns0="http://www.escidoc.de/schemas/commontypes/0.4">
  <namespace uri="http://www.escidoc.de/schemas/usergroup/0.5" prefix="tns"/>
  <namespace prefix="prop" uri="${xsd.soap.common.prop}" default="none" />
  <namespace prefix="srel" uri="${xsd.soap.common.srel}" default="none" />
  <mapping class="de.mpg.escidoc.services.common.valueobjects.intelligent.usergroup.UserGroup" name="user-group" ns="http://www.escidoc.de/schemas/usergroup/0.5">
    <structure name="properties" ns="http://www.escidoc.de/schemas/usergroup/0.5">
      <value style="element" name="creation-date" ns="http://escidoc.de/core/01/properties/" get-method="getPropertiesCreationDate" set-method="setPropertiesCreationDate" usage="optional"/>
       <structure name="created-by" ns="http://escidoc.de/core/01/structural-relations/" usage="optional">
      	<value style="attribute" name="objid" get-method="getPropertiesCreatedBy" set-method="setPropertiesCreatedBy"/>
      </structure>
       <structure name="modified-by" ns="http://escidoc.de/core/01/structural-relations/" usage="optional">
      	<value style="attribute" name="objid" get-method="getPropertiesModifiedBy" set-method="setPropertiesModifiedBy"/>
      </structure>
      <value style="element" name="email" ns="http://escidoc.de/core/01/properties/" get-method="getPropertiesEmail" set-method="setPropertiesEmail" usage="optional"/>
      <value style="element" name="name" ns="http://escidoc.de/core/01/properties/" get-method="getPropertiesName" set-method="setPropertiesName"/>
      <value style="element" name="label" ns="http://escidoc.de/core/01/properties/" get-method="getPropertiesLabel" set-method="setPropertiesLabel"/>
      <value style="element" name="description" ns="http://escidoc.de/core/01/properties/" get-method="getPropertiesDescription" set-method="setPropertiesDescription" usage="optional"/>
      <value style="element" name="type" ns="http://escidoc.de/core/01/properties/" get-method="getPropertiesType" set-method="setPropertiesType" usage="optional"/>
      <value style="element" name="active" ns="http://escidoc.de/core/01/properties/" get-method="getPropertiesActive" set-method="setPropertiesActive" usage="optional"/>
    </structure>
    <structure type="de.mpg.escidoc.services.common.valueobjects.intelligent.usergroup.Selectors" get-method="getSelectors" set-method="setSelectors" usage="optional"/>
    <structure type="de.mpg.escidoc.services.common.valueobjects.intelligent.usergroup.Resources" get-method="getResources" set-method="setResources" usage="optional"/>
    <value style="attribute" name="objid" get-method="getObjid" set-method="setObjid" usage="optional"/>
    <value style="attribute" name="last-modification-date" get-method="getLastModificationDate" set-method="setLastModificationDate" usage="optional"/>
  </mapping>
  <mapping class="de.mpg.escidoc.services.common.valueobjects.intelligent.usergroup.Resources" name="resources" ns="http://www.escidoc.de/schemas/usergroup/0.5">
    <structure map-as="ns0:readOnlyLink" get-method="getCurrentGrants" set-method="setCurrentGrants" name="current-grants" ns="http://www.escidoc.de/schemas/usergroup/0.5"/>
    <value style="attribute" name="last-modification-date" get-method="getLastModificationDate" set-method="setLastModificationDate" usage="optional"/>
  </mapping>
  <mapping class="de.mpg.escidoc.services.common.valueobjects.intelligent.usergroup.Selectors" name="selectors" ns="http://www.escidoc.de/schemas/usergroup/0.5">
    <collection item-type="de.mpg.escidoc.services.common.valueobjects.intelligent.usergroup.Selector" get-method="getSelectors" set-method="setSelectors" create-type="java.util.ArrayList"/>
  </mapping>
  <mapping class="de.mpg.escidoc.services.common.valueobjects.intelligent.usergroup.Selector" name="selector" ns="http://www.escidoc.de/schemas/usergroup/0.5">
    <format type="de.mpg.escidoc.services.common.valueobjects.intelligent.usergroup.Selector$Type" enum-value-method="toString"/>
    <value style="text" get-method="getString" set-method="setString"/>
    <value style="attribute" name="name" get-method="getName" set-method="setName" usage="optional"/>
    <value style="attribute" name="type" get-method="getType" set-method="setType" usage="optional" deserializer="de.mpg.escidoc.services.common.valueobjects.intelligent.usergroup.Selector$Type.convert"/>
    <value style="attribute" name="objid" get-method="getObjid" set-method="setObjid" usage="optional"/>
  </mapping>
</binding>