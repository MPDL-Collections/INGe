<?xml version="1.0" encoding="UTF-8"?>
<Import-Sources
	xmlns="http://escidoc.mpg.de/metadataprofile/schema/0.1/ImportSource"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:dc="http://purl.org/dc/elements/1.1/"
	xmlns:dcterms="http://purl.org/dc/terms/"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://escidoc.mpg.de/metadataprofile/schema/0.1/ImportSource explain-sources.xsd">

	<Import-Source name="arXiv">
		<dc:description>ArXiv is an e-print service in the fields of physics,
			mathematics, non-linear science, computer science, quantitative
			biology and statistics.
		</dc:description>
		<dc:identifier xsi:type="dcterms:URI">http://arxiv.org</dc:identifier>
		<dc:format xsi:type="dcterms:IMT">text/xml</dc:format>
		<dc:format>UTF-8</dc:format>
		<item-url>http://arxiv.org/abs/GETID</item-url>
		<source-identifier>arxiv</source-identifier>
		<source-identifier-example>arXiv:0904.3933</source-identifier-example>
		<harvest-protocol>oai-pmh</harvest-protocol>
		<retry-after>0</retry-after>
		<timeout>0</timeout>
		<number-of-tries>5</number-of-tries>
		<license>http://arxiv.org/help/license</license>
		<status>published</status>
		<MD-fetch-settings>
			<MD-fetch-setting default="true">
				<dc:description>http://arxiv.org/help/prep</dc:description>
				<dc:identifier>http://export.arxiv.org/oai2/oai2?verb=GetRecord%26identifier=oai:arXiv.org:GETID%26metadataPrefix=arXiv
				</dc:identifier>
				<dc:format>application/xml</dc:format>
				<label>arXiv</label>
				<name>arXiv</name>
				<encoding>UTF-8</encoding>
			</MD-fetch-setting>
			<MD-fetch-setting default="false">
				<dc:description>http://www.openarchives.org/OAI/2.0/oai_dc.xsd
				</dc:description>
				<dc:identifier>http://export.arxiv.org/oai2/oai2?verb=GetRecord%26identifier=oai:arXiv.org:GETID%26metadataPrefix=oai_dc
				</dc:identifier>
				<dc:format>application/xml</dc:format>
				<label>oai dc</label>
				<name>oai_dc</name>
				<encoding>UTF-8</encoding>
			</MD-fetch-setting>
		</MD-fetch-settings>
		<FT-fetch-settings>
			<FT-fetch-setting default="true">
				<dc:identifier>http://export.arxiv.org/pdf/GETID</dc:identifier>
				<dc:format>application/pdf</dc:format>
				<label>PDF</label>
				<name>pdf</name>
				<encoding>*</encoding>
				<content-categorie>http://purl.org/escidoc/metadata/ves/content-categories/pre-print
				</content-categorie>
				<visibility>PUBLIC</visibility>
			</FT-fetch-setting>
			<FT-fetch-setting default="false">
				<dc:identifier>http://export.arxiv.org/ps/GETID</dc:identifier>
				<dc:format>application/gzip</dc:format>
				<label>Postscript</label>
				<name>ps</name>
				<encoding>*</encoding>
				<content-categorie>http://purl.org/escidoc/metadata/ves/content-categories/pre-print
				</content-categorie>
				<visibility>PUBLIC</visibility>
			</FT-fetch-setting>
		</FT-fetch-settings>
	</Import-Source>
	
	<Import-Source name="PubMedCentral">
		<dc:description>PubMed Central (PMC) is the U.S. National Institutes
			of Health (NIH) free digital archive of biomedical and life sciences
			journal literature.</dc:description>
		<dc:identifier xsi:type="dcterms:URI">https://www.ncbi.nlm.nih.gov/pmc/
		</dc:identifier>
		<dc:format xsi:type="dcterms:IMT">text/xml</dc:format>
		<dc:format>UTF-8</dc:format>
		<item-url>https://www.ncbi.nlm.nih.gov/pmc/articlerender.fcgi?artid=GETID%26tool=pmcentrez%26rendertype=abstract
		</item-url>
		<source-identifier>pmcid:pmc</source-identifier>
		<source-identifier>pmcid: pmc</source-identifier>
		<source-identifier>pmcid</source-identifier>
		<source-identifier>pmc</source-identifier>
		<source-identifier-example>pmc:2581620</source-identifier-example>
		<harvest-protocol>oai-pmh</harvest-protocol>
		<retry-after>3</retry-after>
		<timeout>0</timeout>
		<number-of-tries>5</number-of-tries>
		<copyright>https://www.ncbi.nlm.nih.gov/pmc/about/copyright.html
		</copyright>
		<status>published</status>
		<MD-fetch-settings>
			<MD-fetch-setting default="true">
				<dc:description>http://www.nlm.nih.gov/bsd/licensee/elements_descriptions.html
				</dc:description>
				<dc:identifier>https://www.ncbi.nlm.nih.gov/pmc/oai/oai.cgi?verb=GetRecord%26identifier=oai:pubmedcentral.nih.gov:GETID%26metadataPrefix=pmc_fm
				</dc:identifier>
				<dc:format>application/xml</dc:format>
				<label>PubMed Central</label>
				<name>pmc</name>
				<encoding>UTF-8</encoding>
			</MD-fetch-setting>
			<MD-fetch-setting default="false">
				<dc:description>http://www.openarchives.org/OAI/2.0/oai_dc.xsd
				</dc:description>
				<dc:identifier>https://www.ncbi.nlm.nih.gov/pmc/oai/oai.cgi?verb=GetRecord%26identifier=oai:pubmedcentral.nih.gov:GETID%26metadataPrefix=oai_dc
				</dc:identifier>
				<dc:format>application/xml</dc:format>
				<label>oai dc</label>
				<name>oai_dc</name>
				<encoding>UTF-8</encoding>
			</MD-fetch-setting>
		</MD-fetch-settings>
		<FT-fetch-settings>
			<FT-fetch-setting default="true">
				<dc:identifier>https://www.ncbi.nlm.nih.gov/pmc/oai/oai.cgi?verb=GetRecord%26identifier=oai:pubmedcentral.nih.gov:GETID%26metadataPrefix=pmc
				</dc:identifier>
				<dc:format>application/pdf</dc:format>
				<label>PDF</label>
				<name>pdf</name>
				<encoding>UTF-8</encoding>
				<content-categorie>http://purl.org/escidoc/metadata/ves/content-categories/any-fulltext
				</content-categorie>
				<visibility>PUBLIC</visibility>
			</FT-fetch-setting>
			<FT-fetch-setting default="false">
				<dc:identifier>https://www.ncbi.nlm.nih.gov/pmc/oai/oai.cgi?verb=GetRecord%26identifier=oai:pubmedcentral.nih.gov:GETID%26metadataPrefix=pmc
				</dc:identifier>
				<dc:format>application/xml</dc:format>
				<label>XML</label>
				<name>xml</name>
				<encoding>UTF-8</encoding>
				<content-categorie>http://purl.org/escidoc/metadata/ves/content-categories/any-fulltext
				</content-categorie>
				<visibility>PUBLIC</visibility>
			</FT-fetch-setting>
		</FT-fetch-settings>		
	</Import-Source>
	
	<Import-Source name="Spires">
		<dc:description>The Stanford Physics Information Retrieval System
			(SPIRES) is a High-Energy Physics Literature Database.
		</dc:description>
		<dc:identifier xsi:type="dcterms:URI">http://www-spires.fnal.gov/spires/
		</dc:identifier>
		<dc:format xsi:type="dcterms:IMT">text/xml</dc:format>
		<dc:format>UTF-8</dc:format>
		<item-url>http://inspirehep.net/search?p=GETID</item-url>
		<source-identifier>spires</source-identifier>
		<source-identifier-example>spires:hep-ph/0001001
		</source-identifier-example>
		<source-identifier-example>spires:arXiv:0812.2902
		</source-identifier-example>
		<source-identifier-example>spires:10.1016/J.NIMA.2008.08.135
		</source-identifier-example>
		<harvest-protocol>http</harvest-protocol>
		<retry-after>3</retry-after>
		<timeout>0</timeout>
		<number-of-tries>5</number-of-tries>
		<status>published</status>
		<MD-fetch-settings>
			<MD-fetch-setting default="true">
				<dc:description>http://www.slac.stanford.edu/spires/hep/api/
				</dc:description>
				<dc:identifier>http://inspirehep.net/search?p=GETID</dc:identifier>
				<dc:format>application/xml</dc:format>
				<label>SPIRES</label>
				<name>spires</name>
				<encoding>UTF-8</encoding>
			</MD-fetch-setting>
		</MD-fetch-settings>
		<FT-fetch-settings>
		</FT-fetch-settings>
	</Import-Source>
	
</Import-Sources>
