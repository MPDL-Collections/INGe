  <tbody style="text-align: left; vertical-align:top;">
	<tr:iterator var="item" value="#{PubItemListSessionBean.currentPartList}" rows="0">
		<h:panelGroup>
		    <tr class="full_area0 listItem">
		      	<td class="free_area0 endline">
		      		<label class="free_area0_p8 endline itemSelectArea">
						<h:selectBooleanCheckbox styleClass="itemCheckBox" value="#{item.selected}" immediate="true" valueChangeListener="#{item.select}" rendered="#{item.showCheckbox}"/>
						&#160;
		      		</label>
		      	</td>
		      	<td class="free_area0 endline status">
		      		<span class="seperator"></span>
		      		<span class="free_area0_p8 status statusArea">
		      		<span class="free_area0 tiny_marginLExcl">
						<h:panelGroup styleClass="big_imgArea statusIcon withdrawnItem" rendered="#{item.isStateWithdrawn}" />
						<h:panelGroup styleClass="big_imgArea statusIcon pendingItem" rendered="#{item.isStatePending}" />
						<h:panelGroup styleClass="big_imgArea statusIcon submittedItem" rendered="#{item.isStateSubmitted}" />
						<h:panelGroup styleClass="big_imgArea statusIcon releasedItem" rendered="#{item.isStateReleased and !item.isStateWithdrawn}" />
						<h:panelGroup styleClass="big_imgArea statusIcon inRevisionItem" rendered="#{item.isStateInRevision}" />
					</span>
							<h:outputLabel styleClass="medium_label endline xTiny_marginLExcl" rendered="#{item.isStateWithdrawn}">
							<h:outputText value="#{item.itemPublicState}" />
						</h:outputLabel>
						<h:outputLabel styleClass="medium_label endline xTiny_marginLExcl" rendered="#{!item.isStateWithdrawn}">
							<h:outputText value="#{item.itemState}" />
						</h:outputLabel>
					</span>
		      	</td>
		      	<td class="free_area0 endline">
		      		<span class="seperator"></span>
		      		<span class="free_area0_p8 itemHeadline">
						<h:outputLink title="#{tip.list_item}" value="#{item.link}">
 							<b><h:outputText value="#{item.shortTitle}" converter="HTMLSubSupConverter" escape="false"/></b>
	 					</h:outputLink>
		      		</span>
		      	</td>
		      	<td class="free_area0 endline">
		      		<span class="seperator"></span>
		      		<span class="large_area0_p8">
		      			<h:outputText value="#{item.creatorsShort}"/>&#160;
		      		</span>
		      	</td>
		      	<td class="free_area0 endline">
		      		<span class="seperator"></span>
		      		<span class="large_area0_p8">
			      		<h:outputText value="#{item.genre}"/>&#160;
			      	</span>
			    </td>
		      	<td class="free_area0 endline">
		      		<span class="seperator"></span>
		      		<span class="large_area0_p8">
		      			<h:outputText value="#{item.latestDate}"/>&#160;
		      		</span>
		      	</td>
		      	<td class="free_area0 endline">
		      		<span class="seperator"></span>
		      		<span class="large_area0_p8 endline filesSection">
		      			<h:outputLink styleClass="large_area0 files" rendered="#{item.numberOfFiles > 0}" title="#{tip.list_fileInfo}" value="#{item.link}#files"><h:outputText value="#{item.fileInfo}"/></h:outputLink>
		      			<h:outputLink styleClass="large_area0 locators" rendered="#{item.numberOfLocators > 0}" title="#{tip.list_locInfo}" value="#{item.link}#locators"><h:outputText value="#{item.locatorInfo}"/></h:outputLink>
		      		</span>
		      	</td>
		    </tr>
		</h:panelGroup>
	</tr:iterator>
  </tbody>
</table>

