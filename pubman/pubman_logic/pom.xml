<?xml version="1.0"?><project>
  <parent>
    <artifactId>pubman</artifactId>
    <groupId>de.mpg.escidoc.pubman</groupId>
    <version>1.0</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>de.mpg.escidoc.pubman</groupId>
  <artifactId>pubman_logic</artifactId>
  <name>pubman_logic</name>
  <packaging>ejb</packaging>
  <version>1.0-SNAPSHOT</version>
  
  <scm>
		<connection>
			scm:svn:http://zim02.gwdg.de/repos/pubman/trunk/pubman/pubman_logic
		</connection>
		<developerConnection>
			scm:svn:https://zim02.gwdg.de/repos/pubman/trunk/pubman/pubman_logic
		</developerConnection>
		<url>http://zim02.gwdg.de/repos/pubman/</url>
	</scm>
  
  <build>
  	<plugins>
  	  <plugin>
        <groupId>org.codehaus.mojo</groupId>
        <artifactId>javacc-maven-plugin</artifactId>
        <executions>
          <execution>
            <id>javacc</id>
            <phase>generate-sources</phase>
            <goals>
              <goal>javacc</goal>
            </goals>
          </execution>
        </executions>
      </plugin>
  	   <plugin>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.5</source>
					<target>1.5</target>
				</configuration>
		</plugin>
		 <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-ejb-plugin</artifactId>
        <configuration>
          <ejbVersion>3.0</ejbVersion>
        </configuration>
      </plugin>
  	</plugins>
  </build>
  
  <dependencies>
    <dependency>
			<groupId>de.mpg.escidoc.services</groupId>
			<artifactId>citationmanager</artifactId>
			<version>1.0-SNAPSHOT</version>
			<scope>compile</scope>
	</dependency>
    <dependency>
			<groupId>de.mpg.escidoc.services</groupId>
			<artifactId>endnotemanager</artifactId>
			<version>1.0-SNAPSHOT</version>
			<scope>compile</scope>
	</dependency>
     <dependency>
			<groupId>de.mpg.escidoc.services</groupId>
			<artifactId>validation</artifactId>
			<version>1.0-SNAPSHOT</version>
			<scope>compile</scope>
	</dependency>
  	<dependency>
			<groupId>de.mpg.escidoc.services</groupId>
			<artifactId>common_logic</artifactId>
			<version>1.0-SNAPSHOT</version>
			<scope>compile</scope>
	</dependency>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>4.1</version>
      <scope>test</scope>
    </dependency>
   </dependencies>
   
</project>
