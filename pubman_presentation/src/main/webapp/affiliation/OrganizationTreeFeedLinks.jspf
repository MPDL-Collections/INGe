<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:f="http://xmlns.jcp.org/jsf/core"
	xmlns:h="http://xmlns.jcp.org/jsf/html"
	xmlns:ui="http://xmlns.jcp.org/jsf/facelets"
	xmlns:p="http://primefaces.org/ui">
	
	<ui:repeat id="detailsDescription" var="topAffiliation"
		value="#{AffiliationBean.topLevelAffs}">
		<link
			href="#{ApplicationBean.pubmanInstanceUrl}/syndication/feed/rss_2.0/publications/organization/#{topAffiliation.reference.objectId}"
			rel="alternate" type="application/rss+xml"
			title="#{lbl.Feed_Recent_Releases}, #{topAffiliation.name}|rss_2.0" />
		<link
			href="#{ApplicationBean.pubmanInstanceUrl}/syndication/feed/atom_1.0/publications/organization/#{topAffiliation.reference.objectId}"
			rel="alternate" type="application/atom+xml"
			title="#{lbl.Feed_Recent_Releases}, #{topAffiliation.name}|atom_1.0" />
	</ui:repeat>

</ui:composition>

