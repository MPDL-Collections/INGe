										for="dateCriterionBean_searchEventStart"
										title="#{lbl.adv_search_lblChkType_event_start_date}" />
							</span> <span class="large_checkbox tiny_marginRIncl"> <h:selectBooleanCheckbox
										id="dateCriterionBean_searchEventEnd"
										value="#{dateCriterionBean.searchEventEnd}" /> <h:outputLabel
										id="lblDdateCriterionBean_searchEventEnd"
										value="#{lbl.adv_search_lblChkType_abb_event_end_date}"
										for="dateCriterionBean_searchEventEnd"
										title="#{lbl.adv_search_lblChkType_event_end_date}" />
							</span>
